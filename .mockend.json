{
  "User":{
  "userName": { "string": {} },
  "fullName": { "string": {} },
  "age": { "int": { "min": 1, "max": 75 } },
  "isActive": { "boolean": {} },
  "addresses":{"hasMany":"Adress"},
  "userAdress":{"belongsTo":"Adress"},
  "email": {"regexp": "#[a-z]{5,10}@[a-z]{5}\\.[a-z]{2,3}"}
  "Adress":{
    "streetName": { "string": {} },
    "BuildingNumber": { "string": {} }
  },
  "Post": {
    "title": { "string": {} },
    "comments": {
      "hasMany": "Comment"
    }
  },
  "Comment": {
    "body": { "string": {} },
    "post": {
      "belongsTo": "Post"
    }
  }
}
